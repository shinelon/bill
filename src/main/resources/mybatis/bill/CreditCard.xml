<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pay.aile.bill.mapper.CreditCardMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.pay.aile.bill.entity.CreditCard">
		<id column="id" property="id" />
		<result column="bank_id" property="bankId" />
		<result column="bill_day" property="billDay" />
		<result column="credits" property="credits" />
		<result column="cash" property="cash" />
		<result column="name" property="name" />
		<result column="numbers" property="numbers" />
		<result column="prepaidCashAmount" property="prepaid_cash_amount" />
        
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, bank_id AS bankId, bill_day AS billDay, credits, cash, name , numbers, prepaid_cash_amount as prepaidCashAmount
    </sql>
   
</mapper>
